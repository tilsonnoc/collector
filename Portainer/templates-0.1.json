{
	"version": "2",
	"templates": [{
			"type": 1,
			"title": "Registry",
			"description": "Docker image registry",
			"categories": [
				"docker"
			],
			"platform": "linux",
			"logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/registry.png",
			"image": "registry:latest",
			"ports": [
				"5000/tcp"
			],
			"volumes": [{
				"container": "/var/lib/registry"
			}]
		},
		{
			"type": 1,
			"title": "Nginx",
			"description": "High performance web server",
			"categories": [
				"webserver"
			],
			"platform": "linux",
			"logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/nginx.png",
			"image": "nginx:latest",
			"ports": [
				"80/tcp",
				"443/tcp"
			],
			"volumes": [{
					"container": "/etc/nginx"
				},
				{
					"container": "/usr/share/nginx/html"
				}
			]
		},
		
		{
			"type": 3,
			"title": "Wordpress -3",
			"description": "Wordpress setup with a MySQL database-3",
			"note": "Deploys a Wordpress instance connected to a MySQL database-3.",
			"categories": [
				"CMS"
			],
			"platform": "linux",
			"logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/wordpress.png",
			"repository": {
				"url": "https://github.com/portainer/templates",
				"stackfile": "stacks/wordpress/docker-compose.yml"
			},
			"env": [{
				"name": "MYSQL_DATABASE_PASSWORD",
				"label": "Database root password",
				"description": "Password used by the MySQL root user."
			}]
		},
		
		{
			"type": 3,
			"title": "MQTT",
			"description": "MQTT Server",
			"note": "For sub and pub for mqtt ",
			"categories": [
				"NOC"
			],
			"platform": "linux",
			"repository": {
				"url": "https://github.com/tilsonnoc/collector",
				"stackfile": "Portainer/stack/MQTT/docker-stack.yml"
			},
			"env": [{
				"name": "env_Name",
				"label": "env_lable",
				"description": "env_description"
			}
			]
		},


